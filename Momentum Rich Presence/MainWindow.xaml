<Window x:Class="Momentum_Rich_Presence.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Momentum_Rich_Presence"
        mc:Ignorable="d"
        Title="Momentum Rich Presence" Height="114.341" Width="395.472">
    <Grid Background="#FF1F1F1F">
        <Button x:Name="Inject" Content="Inject" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="140" Foreground="White" Background="#FF363636" Height="31" BorderBrush="{x:Null}" Click="Inject_Click"/>
        <Label x:Name="Status" Content="Currently not injected" HorizontalAlignment="Left" Height="30" Margin="10,41,0,0" VerticalAlignment="Top" Width="219" Foreground="#FFF92672"/>
        <Label x:Name="Map" Content="Map: ?" HorizontalAlignment="Left" Height="30" Margin="162,11,0,0" VerticalAlignment="Top" Width="219" Foreground="#FF75715E"/>
        <Label x:Name="Speed" Content="Speed: ?" HorizontalAlignment="Left" Height="30" Margin="162,41,0,0" VerticalAlignment="Top" Width="219" Foreground="#FF75715E"/>

    </Grid>
</Window>
